{
  "vimft": {
    "prefix": "vimft",
    "description": "Add a hint for the filetype to VIM",
    "body": [
      "# vim:set filetype=something:"
    ]
  },
  "uuid": {
    "prefix": "uuid",
    "description": "Insert a random UUID",
    "body": [
      "$UUID"
    ]
  },
  "today": {
    "prefix": "today",
    "description": "Insert current date",
    "body": [
      "$CURRENT_YEAR-$CURRENT_MONTH-${CURRENT_DATE}T$CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND$CURRENT_TIMEZONE_OFFSET"
    ]
  }
}
